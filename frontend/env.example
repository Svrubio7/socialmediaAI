# Copy this file to .env and fill in your values
# (Supabase docs use SUPABASE_URL / SUPABASE_KEY; in Nuxt we use the names below.)
#
# Same-origin API base path used by the unified app runtime.
NUXT_PUBLIC_API_URL=/api/v1

# Supabase – from Dashboard: Project Settings → API (Project URL + anon/publishable key)
NUXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NUXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-or-publishable-key
# Single private bucket for direct uploads: videos and thumbnails go here (paths: videos/{uid}/, thumbnails/{uid}/)
NUXT_PUBLIC_SUPABASE_STORAGE_BUCKET=videos
NUXT_PUBLIC_EDITOR_PARITY_FLAG=false
NUXT_PUBLIC_EDITOR_FORK_ENABLED=false
NUXT_PUBLIC_EDITOR_FORK_ROLLOUT_PERCENT=0
NUXT_PUBLIC_EDITOR_FORK_ALLOWLIST=
